{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ],
    "targets": {
      "production": {
        "hosting": {
          "websapmax": {}
        }
      },
      "staging": {
        "hosting": {
          "websapmax-staging": {}
        }
      }
    }
  }
}
